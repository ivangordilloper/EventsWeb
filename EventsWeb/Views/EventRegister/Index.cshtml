@model IEnumerable<EventsWeb.Models.Eventregister>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Evaluation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comments)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdeventscheduleNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IduserNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Evaluation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdeventscheduleNavigation.Ideventschedule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IduserNavigation.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ideventregister">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ideventregister">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ideventregister">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
